  "version": "7.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngneat/until-destroy.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngneat/until-destroy#readme",
  "bugs": {
    "url": "https://github.com/ngneat/until-destroy/issues"
  },
  "maintainers": [
    "Netanel Basal",
    "Artur Androsovych"
  ],
  "keywords": [
    "Angular easy unsubscribe",
    "RxJS easy unsubscribe",
